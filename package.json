{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "i18next": "^21.8.13",
    "inversify": "^6.0.1",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.1",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.0",
    "react-scripts": "5.0.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prestart": "yarn build:configs",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:scripts": "tsc ./scripts/*.ts --outDir ./scripts/build",
    "build:configs": "tsc ./configs/*.config.ts --outDir ./configs/build && node ./scripts/build/build-config.js",
    "postinstall": "yarn build:scripts && yarn build:configs",
    "tsc-cli": "tsc",
    "make:feature": "node ./scripts/build/make-feature.js --name "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/yargs": "^17.0.10",
    "change-case": "^4.1.2",
    "yargs": "^17.5.1"
  }
}
